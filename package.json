{
  "name": "zonemaster-gui",
  "version": "3.5.0",
  "license": "BSD-2-Clause",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node scripts/faq.js && ng build --prod",
    "release": "npm run build && node scripts/create_release.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "playwright test",
    "e2e:update": "playwright test --update-snapshots",
    "e2e:install": "playwright install chromium",
    "e2e:install-deps": "playwright install-deps chromium",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --sort --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{da,en,es,fi,fr,nb,sv}.json --null-as-default-value --clean --sort --format json --format-indentation '    '"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/localize": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/resize-observer-browser": "^0.1.6",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "github-fork-ribbon-css": "^0.2.1",
    "rxjs": "^6.6.0",
    "showdown": "^1.9.1",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@playwright/test": "^1.18.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.4.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^13.11.0",
    "archiver": "^3.1.1",
    "browserstack-local": "^1.3.7",
    "codelyzer": "^5.0.1",
    "david": "^12.0.0",
    "i": "^0.3.7",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "ngx-translate-lint": "^1.8.0",
    "ntypescript": "^1.201706190042.1",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.1",
    "typescript": "^4.0.5"
  }
}
